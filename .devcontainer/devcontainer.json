{
    "remoteUser": "ros",
    "name": "ros-devcontainer",
    "updateRemoteUserUID": false, 
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/home/ros/proj/dev_ws",
    "customizations": {
        "vscode": {
            "extensions": [
				"ms-vscode.cpptools",
				"ms-vscode.cmake-tools",
				"ms-vscode.cpptools-extension-pack",
				"ms-python.python",
                "ms-iot.vscode-ros",
                "redhat.vscode-xml"
			]
        }
    },
    "shutdownAction": "stopCompose"
}